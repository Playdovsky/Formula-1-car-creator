<Window x:Class="Main.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Main"
        mc:Ignorable="d"
        Title="MainWindow" Height="549" Width="800">
    <Grid Margin="0,0,0,-16">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ContentControl x:Name="ContentControl" Content="" HorizontalAlignment="Center" Height="440" Margin="0,75,0,0" VerticalAlignment="Top" Width="780"/>
        <Button x:Name="ButtonCarList" Content="GARAGE" HorizontalAlignment="Left" Height="40" Margin="10,30,0,0" VerticalAlignment="Top" Width="80" Click="ButtonCarList_Click"/>
        <Button x:Name="ButtonCarType" Content="CHANGE CAR TYPE" HorizontalAlignment="Left" Height="40" Margin="95,30,0,0" VerticalAlignment="Top" Width="120" Click="ButtonCarType_Click"/>
    </Grid>
</Window>
